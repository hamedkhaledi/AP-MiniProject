                              </RadioButton>
                              <RadioButton fx:id="Food12" mnemonicParsing="false" text="RadioButton" toggleGroup="$SaturdayFood" />
                           </children>
                        </VBox>
                        <VBox layoutX="261.0" layoutY="177.0" prefHeight="36.0" prefWidth="100.0">
